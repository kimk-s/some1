{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Some1.Play.Server.Tcp": "Information"
    }
  },
  "Listener": {
    "Port": 8000
  },
  "Data": {
    "InMemory": false
  },
  "Auth": {
    "Fake": false
  },
  "PlayLoop": {
    "FPS": 30,
    "MinFPS": 1,
    "TimeScale": 1,
    "LogOnOverRate": 0.1
  },
  "Play": {
    "Parallel": {
      "Count": 0
    },
    "Players": {
      "Count": 200,
      "Busy": {
        "PauseThreshold": 1,
        "ResumeThreshold": 0.95
      },
      "LoadThrottle": 0,
      "SaveThrottle": 0.2
    }
  }
}
